{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/(.*)": "<rootDir>/../src/$1",
    "@app": "<rootDir>/../src",
    "@app/auth/(.*)": "<rootDir>/../libs/auth/src/$1",
    "@app/auth": "<rootDir>/../libs/auth/src",
    "@app/firebase/(.*)": "<rootDir>/../libs/firebase/src/$1",
    "@app/firebase": "<rootDir>/../libs/firebase/src"
  }
}